{
    "source": {
        "include" : ["./src/"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "opts": {
        "encoding": "utf8",              
        "destination": "docs/",          
        "recurse": true,   
        "verbose": true              
    }
}